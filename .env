# .env.example - Copy to .env and fill in your actual values for local development
# For GCP deployment, set these in the Cloud Function's environment variables settings.

# --- GCP Configuration ---
GCP_PROJECT_ID="your-gcp-project-id"

# --- Kite Connect API Keys ---
# Store these securely, preferably in Secret Manager for GCP deployments
KITE_API_KEY="your_kite_api_key"
KITE_API_SECRET="your_kite_api_secret"
# KITE_ACCESS_TOKEN is dynamic and needs to be generated daily. 
# Store it here for local dev after generation, or have a robust refresh mechanism.
KITE_ACCESS_TOKEN="" 

# --- News API Key ---
NEWS_API_KEY="your_newsapi_org_key"

# --- Gemini API Key ---
# Store securely, preferably in Secret Manager for GCP deployments
GEMINI_API_KEY="your_google_gemini_api_key"


# --- Pub/Sub Topic Names (Examples - use your actual topic names) ---
PUBSUB_MARKET_TICKS_TOPIC="market-ticks-tradeveaver"
PUBSUB_INSTRUMENT_SELECTION_TOPIC="instrument-selection-tradeveaver"
PUBSUB_STRATEGY_SIGNALS_TOPIC="strategy-signals-tradeveaver" # Main Agent subscribes here
PUBSUB_NEWS_SENTIMENT_TOPIC="news-sentiment-tradeveaver" # Market Selector & Main Agent might subscribe
PUBSUB_KITE_TICKER_CONTROL_TOPIC="kite-ticker-control-tradeveaver" # For Market Selector to tell Ticker Service what to subscribe

# --- Firestore Collection Names (Examples) ---
FS_COLLECTION_AGENT_CONFIG="agent_configs_v4_ai"
FS_COLLECTION_ACTIVE_TRADES="active_trades_v4_ai"
FS_COLLECTION_TRADE_HISTORY="trade_history_v4_ai"
FS_DOC_ID_MAIN_AGENT_CONFIG="main_agent_settings_v4"


# --- AI Agent Specific Config (Examples - can also be in Firestore) ---
# For BaseAIStrategyAgent and its children
AI_STRATEGY_CANDLE_HISTORY="45"
AI_STRATEGY_MIN_CANDLES="25"
AI_STRATEGY_DEFAULT_MODEL="gemini-1.5-pro-latest" # Or gemini-1.5-flash-latest for cost/speed

# For Main AI Agent
MAIN_AI_DECISION_MODEL="gemini-1.5-pro-latest"
MAIN_AI_PROFIT_TARGET_PCT="0.10"  # 10%
MAIN_AI_STOP_LOSS_RISK_PCT="0.20" # 20%
MAIN_AI_CAPITAL_ALLOC_PCT="0.10"  # 10%
MAIN_AI_MAX_SUCCESSFUL_TRADES_PER_DAY="5"
# ... other main agent specific thresholds ...

# --- Backtesting Configuration (Examples) ---
BT_HISTORICAL_DATA_OUTPUT_DIR="historical_data_store"
BT_AI_MODE="mock_prefer" # "mock_only", "mock_prefer", "live_only"
